<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Responsive Admin &amp; Dashboard Template based on Bootstrap 5">
    <meta name="author" content="AdminKit">
    <meta name="keywords" content="adminkit, bootstrap, bootstrap 5, admin, dashboard, template, responsive, css, sass, html, theme, front-end, ui kit, web">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="shortcut icon" href="~/img/icons/icon-48x48.png" />

    <link rel="stylesheet" href="~/Plugins/fontawesome-free/css/all.min.css"/>
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />



    <link rel="canonical" href="https://demo-basic.adminkit.io/" />

    <title>E-vent Online Ticket</title>

    <link href="~/css/app.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;600&display=swap" rel="stylesheet">
</head>

<body>
    <div class="wrapper">
        <nav id="sidebar" class="sidebar js-sidebar">
            <div class="sidebar-content js-simplebar">
                <a class="sidebar-brand" href="index.html">
                    <span class="align-middle">E-vent Admin</span>
                </a>

                <ul class="sidebar-nav">
                    <li class="sidebar-header">
                        Pages
                    </li>

                    <li class="sidebar-item">
                        <a class="sidebar-link" href="index">
                            <i class="align-middle" data-feather="sliders"></i> <span class="align-middle">Dashboard</span>
                        </a>
                    </li>

                    <li class="sidebar-item">
                        <a class="sidebar-link" href="@Url.Action("Users","Admin")">
                            <i class="align-middle" data-feather="user"></i> <span class="align-middle">Users</span>
                        </a>
                    </li>

                    <li class="sidebar-item">
                        <a class="sidebar-link" href="@Url.Action("Events","Admin")">
                            <i class="align-middle" data-feather="check-square"></i> <span class="align-middle">Events</span>
                        </a>
                    </li>

                    <li class="sidebar-item">
                        <a class="sidebar-link" href="@Url.Action("Categories","Admin")">
                            <i class="align-middle" data-feather="grid"></i> <span class="align-middle">Categories</span>
                        </a>
                    </li>


                    <li class="sidebar-item">
                        <a class="sidebar-link" href="@Url.Action("Cities","Admin")">
                            <i class="align-middle" data-feather="map"></i> <span class="align-middle">Cities</span>
                        </a>
                    </li>

                    <li class="sidebar-item">
                        <a class="sidebar-link" href="@Url.Action("Logout","Admin")">
                            <i class="align-middle" data-feather="log-in"></i> <span class="align-middle">Logout</span>
                        </a>
                    </li>

                </ul>
            </div>
        </nav>

        <div class="main">
            <nav class="navbar navbar-expand navbar-light navbar-bg">
                <a class="sidebar-toggle js-sidebar-toggle">
                    <i class="hamburger align-self-center"></i>
                </a>

                <div class="navbar-collapse collapse">
                    <ul class="navbar-nav navbar-align">


                        <li>
                            <a>
                                <img src="~/img/avatars/pp.png" class="avatar img-fluid rounded me-1" alt="Mazlum Kahraman" /> <span class="text-dark">Mazlum Kahraman</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>

            <main class="content">
                <div class="container-fluid p-0">
                    @RenderBody()
                </div>
            </main>

            <footer class="footer">
                <div class="container-fluid">
                    <div class="row text-muted">
                        <div class="col-6 text-start">
                            <p class="mb-0">
                                <a class="text-muted"><strong>E-vent Online Ticket</strong></a> &copy 2022 <a class="text-muted"><strong>Mazlum Kahraman</strong></a>
                            </p>
                        </div>
                        <div class="col-6 text-end">
                            <ul class="list-inline">
                                <li class="list-inline-item">
                                    <a class="text-muted">Support</a>
                                </li>
                                <li class="list-inline-item">
                                    <a class="text-muted">Help Center</a>
                                </li>
                                <li class="list-inline-item">
                                    <a class="text-muted">Privacy</a>
                                </li>
                                <li class="list-inline-item">
                                    <a class="text-muted">Terms</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </footer>

        </div>
    </div>

    <script src="~/js/app.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>

    @RenderSection("Scripts",false)

</body>

</html>